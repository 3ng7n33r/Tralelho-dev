#: translation/templates/translation/language.html:3545
#: translation/templates/translation/language.html:3548
msgid "Je n’ai pas reçu de coup récemment"
msgstr "I have not been hit/received any blows recently"

#: translation/templates/translation/language.html:3561
#: translation/templates/translation/language.html:3564
msgid "J’ai été piqué ou mordu récemment"
msgstr "I was stung/bitten recently"

#: translation/templates/translation/language.html:3569
#: translation/templates/translation/language.html:3572
msgid "Je n’ai pas été piqué ou mordu récemment"
msgstr "I wasn't stung/bitten recently"

#: translation/templates/translation/language.html:3577
#: translation/templates/translation/language.html:3580
msgid "Ca me démange"
msgstr "It itches"

#: translation/templates/translation/language.html:3585
#: translation/templates/translation/language.html:3588
msgid "Ca ne me démange pas"
msgstr "It doesn't itch"

#: translation/templates/translation/language.html:3593
#: translation/templates/translation/language.html:3596
msgid "Ma douleur est :"
msgstr "my pain is:"

#: translation/templates/translation/language.html:3601
#: translation/templates/translation/language.html:3604
msgid "Faible"
msgstr "weak"

#: translation/templates/translation/language.html:3609
#: translation/templates/translation/language.html:3612
msgid "Moyenne"
msgstr "average"

#: translation/templates/translation/language.html:3617
#: translation/templates/translation/language.html:3620
msgid "Forte"
msgstr "strong"

#: translation/templates/translation/language.html:3625
#: translation/templates/translation/language.html:3628
msgid "Insupportable"
msgstr "unbearable"

#: translation/templates/translation/language.html:3633
#: translation/templates/translation/language.html:3636
msgid "Des clips intra-cranien"
msgstr "intracranial clips"

#: translation/templates/translation/language.html:3641
#: translation/templates/translation/language.html:3644
msgid "Un implant cochléaire"
msgstr "a cochlear implant"

#: translation/templates/translation/language.html:3649
#: translation/templates/translation/language.html:3652
msgid "Un implant occulaire"
msgstr "an intraocular lens implant"

#: translation/templates/translation/language.html:3657
#: translation/templates/translation/language.html:3660
msgid "Du métal dans le corps"
msgstr "Some metal in the body"

#: translation/templates/translation/language.html:3665
#: translation/templates/translation/language.html:3668
msgid "Un défibrillateur"
msgstr "A defibrillator"

#: translation/templates/translation/language.html:3673
#: translation/templates/translation/language.html:3676
msgid "Une chambre implantable"
msgstr "a PICC line (Peripherally Inserted Central Catheter)"

#: translation/templates/translation/language.html:3681
#: translation/templates/translation/language.html:3684
msgid "Allaitez-vous ?"
msgstr "Are you breastfeeding? Do you breastfeed?"

#: translation/templates/translation/language.html:3689
#: translation/templates/translation/language.html:3692
msgid "Où se trouve le médecin le plus proche ?"
msgstr "Where is the closest doctor?"

#: translation/templates/translation/language.html:3697
#: translation/templates/translation/language.html:3700
msgid "Où se trouve l'hôpital le plus proche ?"
msgstr "Where is the closest hospital?"

#: translation/templates/translation/language.html:3705
#: translation/templates/translation/language.html:3708
msgid "Où se trouve la pharmacie la plus proche ?"
msgstr "Where is the closest pharmacy?"

#: translation/templates/translation/language.html:3713
#: translation/templates/translation/language.html:3716
msgid "J'ai besoin de secours, pouvez-vous les appeler ?"
msgstr "I need (medical) assistance, could you please call an ambulance?"

#: translation/templates/translation/language.html:3721
#: translation/templates/translation/language.html:3724
msgid "Une IRM"
msgstr "a MRI (Magnetic resonance imaging)"

#: translation/templates/translation/language.html:3729
#: translation/templates/translation/language.html:3732
msgid "L'examen est long. Il dure entre 15 et 40 minutes"
msgstr "The scan takes a long time. About 15 to 40 minutes in total."

#: translation/templates/translation/language.html:3737
#: translation/templates/translation/language.html:3740
msgid "L'examen n'est pas douloureux mais il est très bruyant"
msgstr "The scan isn’t painful but it is very loud"

#: translation/templates/translation/language.html:3745
#: translation/templates/translation/language.html:3748
msgid "Il faut rester immobile pendant toute la durée de l'examen"
msgstr ""

#: translation/templates/translation/language.html:3753
#: translation/templates/translation/language.html:3756
msgid "Des éclats métalliques dans les yeux?"
msgstr "Do you have metal shards in your eyes ?"

#: translation/templates/translation/language.html:3761
#: translation/templates/translation/language.html:3764
msgid "une valve mécanique"
msgstr "Mechanical heart valve"

#: translation/templates/translation/language.html:3769
#: translation/templates/translation/language.html:3772
msgid "un capteur de glycémie"
msgstr "a bloodsugar sensor"

#: translation/templates/translation/language.html:3777
#: translation/templates/translation/language.html:3780
msgid "si oui, avez-vous une carte avec les références du matériel?"
msgstr "If yes, do you have a card containing the equipment reference numbers?"

#: translation/templates/translation/language.html:3785
#: translation/templates/translation/language.html:3788
msgid "Quelle est votre taille et votre poids?"
msgstr "What is your height and weight"

#: translation/templates/translation/language.html:3793
#: translation/templates/translation/language.html:3796
msgid "Durant l'examen, on vous demandera de gonfler et les poumons et de bloquer la respiration"
msgstr "During the scan/examination, we will ask you to breath in deeply and keep your lungs filled with air"

#: translation/templates/translation/language.html:3801
#: translation/templates/translation/language.html:3804
msgid "Nous allons injecter un produit de contraste qui va vous donner chaud dans tout le corps, c'est normal ne vous inquiétez pas"
msgstr "We will inject you with intravenous dye which will create a warm feeling in your body. This reaction is normal and you should not worry about it"

#: translation/templates/translation/language.html:3809
#: translation/templates/translation/language.html:3812
msgid "Prenez-vous un traitement pour la thyroïde ?"
msgstr "Are you following a treatment for thyroid?"

#: translation/templates/translation/language.html:3817
#: translation/templates/translation/language.html:3820
msgid "Avez-vous des problèmes rénaux ?"
msgstr "Do you have kidney problems/issues ?"

#: translation/templates/translation/language.html:3825
#: translation/templates/translation/language.html:3828
msgid "Pouvez-vous signer l’autorisation de soins ci-dessous ?"
msgstr "Could you please sign the authorization for medical treatment ?"

#: translation/templates/translation/language.html:3833
#: translation/templates/translation/language.html:3836
msgid "Nom :"
msgstr "Last Name :"

#: translation/templates/translation/language.html:3841
#: translation/templates/translation/language.html:3844
msgid "Prénom :"
msgstr "First Name :"

#: translation/templates/translation/language.html:3849
#: translation/templates/translation/language.html:3852
msgid "Date de naissance :"
msgstr "Birth date :"

#: translation/templates/translation/language.html:3857
#: translation/templates/translation/language.html:3860
msgid "La taille :"
msgstr "Height :"

#: translation/templates/translation/language.html:3865
#: translation/templates/translation/language.html:3868
msgid "Poids :"
msgstr "Weight :"

#: translation/templates/translation/language.html:3889
#: translation/templates/translation/language.html:3892
msgid "Je ne sais pas"
msgstr "I don't know"

