#: translation/templates/translation/documents.html:9
#: translation/templates/translation/index.html:9
#: translation/templates/translation/language.html:9
#: translation/templates/translation/support.html:9
msgid "Soutenez-nous"
msgstr "Support us"

#: translation/templates/translation/about.html:10
#: translation/templates/translation/disclaimer.html:10
#: translation/templates/translation/docindex.html:10
