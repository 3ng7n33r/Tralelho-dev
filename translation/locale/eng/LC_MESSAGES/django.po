#: translation/templates/translation/language.html:690
msgid "Avez vous des fourmillement ? Montrez moi où"
msgstr ""

#: translation/templates/translation/language.html:695
#: translation/templates/translation/language.html:698
msgid "Poussez sur mes mains"
msgstr "Push on my hands"

#: translation/templates/translation/language.html:703
#: translation/templates/translation/language.html:706
msgid "Ouvrez les yeux"
msgstr "Open your eyes"

#: translation/templates/translation/language.html:711
#: translation/templates/translation/language.html:714
msgid "Ouvrez la bouche"
msgstr "Open your mouth."

#: translation/templates/translation/language.html:719
#: translation/templates/translation/language.html:722
msgid "Levez le bras droit"
msgstr "Lift your right arm"

#: translation/templates/translation/language.html:727
#: translation/templates/translation/language.html:730
msgid "Avez-vous des douleurs à la tête ?"
msgstr ""

#: translation/templates/translation/language.html:735
#: translation/templates/translation/language.html:738
#: translation/templates/translation/language.html:2697
#: translation/templates/translation/language.html:2700
msgid "La douleur est apparue :"
msgstr "The pain came on:"

#: translation/templates/translation/language.html:743
#: translation/templates/translation/language.html:746
#: translation/templates/translation/language.html:2713
#: translation/templates/translation/language.html:2716
msgid "Progressivement"
msgstr "little by little"

#: translation/templates/translation/language.html:751
#: translation/templates/translation/language.html:754
#: translation/templates/translation/language.html:2705
#: translation/templates/translation/language.html:2708
msgid "Brutalement"
msgstr "suddenly"

#: translation/templates/translation/language.html:759
#: translation/templates/translation/language.html:762
msgid "Avez vous des douleurs dans la nuque ?"
msgstr ""

#: translation/templates/translation/language.html:767
#: translation/templates/translation/language.html:770
msgid "Avez-vous voyagez ces derniers mois ?"
msgstr ""

#: translation/templates/translation/language.html:775
#: translation/templates/translation/language.html:778
msgid "Dans quel pays ?"
msgstr "Have you travelled overseas? Where to?"

#: translation/templates/translation/language.html:783
#: translation/templates/translation/language.html:786
msgid "Est ce que la lumière vous dérange ?"
msgstr ""

#: translation/templates/translation/language.html:791
#: translation/templates/translation/language.html:794
msgid "Est ce que le bruit vous dérange ?"
msgstr ""

#: translation/templates/translation/language.html:799
#: translation/templates/translation/language.html:802
msgid "Je vais faire une piqûre au bout de votre doigt pour connaître votre taux de sucre"
msgstr ""

#: translation/templates/translation/language.html:808
msgid "PNEUMOLOGIE"
msgstr ""

#: translation/templates/translation/language.html:814
#: translation/templates/translation/language.html:817
msgid "Je vais poser ma main sur votre ventre pour compter votre respiration, respirez normalement et ne parlez pas pendant ce temps"
msgstr ""

#: translation/templates/translation/language.html:822
#: translation/templates/translation/language.html:825
msgid "Etes-vous essoufflé ?"
msgstr ""

#: translation/templates/translation/language.html:830
#: translation/templates/translation/language.html:833
msgid "Prenez une grande inspiration puis bloquez votre respiration"
msgstr "Take a deep breath in and hold it"

#: translation/templates/translation/language.html:838
#: translation/templates/translation/language.html:841
msgid "Respirez normalement"
msgstr "Breathe normally"

#: translation/templates/translation/language.html:846
#: translation/templates/translation/language.html:849
msgid "Respirez profondément"
msgstr "Breathe deeply"

#: translation/templates/translation/language.html:854
#: translation/templates/translation/language.html:857
msgid "Fumez-vous ?"
msgstr "Do you smoke?"

#: translation/templates/translation/language.html:862
#: translation/templates/translation/language.html:865
msgid "Etes-vous asthmatique ?"
msgstr ""

#: translation/templates/translation/language.html:870
#: translation/templates/translation/language.html:873
msgid "Avez vous pris votre traitement contre l’asthme ?"
msgstr ""

#: translation/templates/translation/language.html:878
#: translation/templates/translation/language.html:881
msgid "Avez-vous respiré des fumées ?"
msgstr ""

#: translation/templates/translation/language.html:886
#: translation/templates/translation/language.html:889
msgid "Je vous laisse vous moucher, pour savoir s’il y a des traces de suie"
msgstr ""

#: translation/templates/translation/language.html:895
msgid "CARDIOLOGIE"
msgstr ""

#: translation/templates/translation/language.html:901
#: translation/templates/translation/language.html:904
msgid "Est-ce que votre douleur :"
msgstr "Describe your pain."

#: translation/templates/translation/language.html:909
#: translation/templates/translation/language.html:912
#: translation/templates/translation/language.html:3001
#: translation/templates/translation/language.html:3004
msgid "Serre"
msgstr "Is it a tight sensation?"

#: translation/templates/translation/language.html:917
#: translation/templates/translation/language.html:920
#: translation/templates/translation/language.html:2993
#: translation/templates/translation/language.html:2996
msgid "Pique"
msgstr "Is it a sharp sensation?"

#: translation/templates/translation/language.html:925
#: translation/templates/translation/language.html:928
#: translation/templates/translation/language.html:3009
#: translation/templates/translation/language.html:3012
msgid "Brûle"
msgstr "Is it a burning sensation?"

#: translation/templates/translation/language.html:933
#: translation/templates/translation/language.html:936
msgid "Est-ce que votre douleur irradie ? Montrez-moi vers où ?"
msgstr ""

#: translation/templates/translation/language.html:941
#: translation/templates/translation/language.html:944
msgid "Depuis combien de temps avez vous cette douleur ?"
msgstr ""

#: translation/templates/translation/language.html:949
#: translation/templates/translation/language.html:952
msgid "Minutes"
msgstr "Minutes"

#: translation/templates/translation/language.html:957
#: translation/templates/translation/language.html:960
msgid "Heures"
msgstr "hours"

#: translation/templates/translation/language.html:965
#: translation/templates/translation/language.html:968
msgid "Jours"
msgstr "days"

#: translation/templates/translation/language.html:973
#: translation/templates/translation/language.html:976
msgid "Je vais prendre votre pouls"
msgstr "I am going to take your pulse"

#: translation/templates/translation/language.html:981
#: translation/templates/translation/language.html:984
msgid "Je vais appuyer doucement sur votre ongle"
msgstr "I am going to press lightly on your nail"

#: translation/templates/translation/language.html:989
#: translation/templates/translation/language.html:992
