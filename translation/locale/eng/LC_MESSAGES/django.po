#: translation/templates/doctemplates/irm.html:23
#: translation/templates/doctemplates/irm.html:24
msgid "Vous allez passer une IRM"
msgstr "You are going to have an MRI"

#: translation/templates/doctemplates/irm.html:26
#: translation/templates/doctemplates/irm.html:27
msgid "Pour entrer dans la salle d’IRM, il faut retirer tout objet métallique (appareils dentaires et auditifs, montre, bijoux...)"
msgstr "To enter the MRI room, you must remove any metallic objects (dental and hearing aids, watches, jewellery, ...)"

#: translation/templates/doctemplates/irm.html:29
#: translation/templates/doctemplates/irm.html:30
msgid "Une perfusion va vous être posée pour injecter un produit de contraste au cours de l’examen"
msgstr "An infusion will be placed to inject a contrast medium during the examination"

#: translation/templates/doctemplates/irm.html:32
#: translation/templates/doctemplates/irm.html:33
#: translation/templates/doctemplates/scn.html:32
#: translation/templates/doctemplates/scn.html:33
msgid "Il faudra rester immobile pendant tout l’examen"
msgstr "You will have to keep still during the whole examination"

#: translation/templates/doctemplates/irm.html:35
#: translation/templates/doctemplates/irm.html:36
msgid "L'examen est long. Il dure entre 15 et 40 minutes. L'examen n'est pas douloureux mais il est très bruyant"
msgstr "The exam is long. It takes between 15 and 40 minutes. The exam is not painful but it is noisy"

#: translation/templates/doctemplates/irm.html:38
#: translation/templates/doctemplates/irm.html:39
#: translation/templates/doctemplates/scn.html:35
#: translation/templates/doctemplates/scn.html:36
#: translation/templates/translation/language.html:3601
#: translation/templates/translation/language.html:3604
msgid "Durant l'examen, on vous demandera de gonfler et les poumons et de bloquer la respiration"
msgstr "During the scan/examination, we will ask you to breathe in deeply and hold your breath"

#: translation/templates/doctemplates/irm.html:41
#: translation/templates/doctemplates/irm.html:42
#: translation/templates/translation/language.html:1971
#: translation/templates/translation/language.html:1974
msgid "Pour nous appeler appuyer ici"
msgstr "To call us, press here"

#: translation/templates/doctemplates/irm.html:47
msgid "Checklist IRM"
msgstr "Checklist MRI"

#: translation/templates/doctemplates/irm.html:51
#: translation/templates/doctemplates/irm.html:52
#: translation/templates/doctemplates/scn.html:47
#: translation/templates/doctemplates/scn.html:48
msgid "Nom"
msgstr "Name"

#: translation/templates/doctemplates/irm.html:53
#: translation/templates/doctemplates/irm.html:54
#: translation/templates/doctemplates/scn.html:49
#: translation/templates/doctemplates/scn.html:50
msgid "Prénom"
msgstr "First name"

#: translation/templates/doctemplates/irm.html:55
#: translation/templates/doctemplates/irm.html:56
#: translation/templates/doctemplates/scn.html:51
#: translation/templates/doctemplates/scn.html:52
msgid "Date de naissance"
msgstr "Date of birth"

#: translation/templates/doctemplates/irm.html:57
#: translation/templates/doctemplates/irm.html:58
#: translation/templates/doctemplates/scn.html:53
#: translation/templates/doctemplates/scn.html:54
msgid "Taille"
msgstr "Size"

#: translation/templates/doctemplates/irm.html:59
#: translation/templates/doctemplates/irm.html:60
#: translation/templates/doctemplates/scn.html:55
#: translation/templates/doctemplates/scn.html:56
msgid "Poids"
msgstr "Weight"

#: translation/templates/doctemplates/irm.html:78
#: translation/templates/doctemplates/irm.html:79
#: translation/templates/doctemplates/irm.html:140
#: translation/templates/doctemplates/irm.html:141
#: translation/templates/doctemplates/scn.html:73
#: translation/templates/doctemplates/scn.html:74
#: translation/templates/translation/language.html:336
#: translation/templates/translation/language.html:339
#: translation/templates/translation/language.html:3681
#: translation/templates/translation/language.html:3684
msgid "Oui"
msgstr "Yes"

#: translation/templates/doctemplates/irm.html:80
#: translation/templates/doctemplates/irm.html:81
#: translation/templates/doctemplates/irm.html:142
#: translation/templates/doctemplates/irm.html:143
#: translation/templates/doctemplates/scn.html:75
#: translation/templates/doctemplates/scn.html:76
#: translation/templates/translation/language.html:344
#: translation/templates/translation/language.html:347
#: translation/templates/translation/language.html:3689
#: translation/templates/translation/language.html:3692
msgid "Non"
msgstr "No"

#: translation/templates/doctemplates/irm.html:82
#: translation/templates/doctemplates/irm.html:83
#: translation/templates/doctemplates/irm.html:144
#: translation/templates/doctemplates/irm.html:145
#: translation/templates/doctemplates/scn.html:77
#: translation/templates/doctemplates/scn.html:78
#: translation/templates/translation/language.html:3697
#: translation/templates/translation/language.html:3700
msgid "Je ne sais pas"
msgstr "I don't know"

#: translation/templates/doctemplates/irm.html:89
#: translation/templates/doctemplates/irm.html:90
msgid "Avez-vous un pace-maker?"
msgstr "Do you have a pacemaker?"

#: translation/templates/doctemplates/irm.html:96
#: translation/templates/doctemplates/irm.html:97
msgid "Avez-vous une valve cardiaque?"
msgstr "Do you have a heart valve replacement?"

#: translation/templates/doctemplates/irm.html:103
#: translation/templates/doctemplates/irm.html:104
msgid "Avez-vous des stents coronaires?"
msgstr "Do you have a coronary stent?"

#: translation/templates/doctemplates/irm.html:111
#: translation/templates/doctemplates/irm.html:112
msgid "Avez-vous un défibrillateur?"
msgstr "Do you have an implanted defibrillator?"

#: translation/templates/doctemplates/irm.html:118
#: translation/templates/doctemplates/irm.html:119
msgid "Avez-vous une chambre implantable?"
msgstr "Do you have an implanted port?"

#: translation/templates/doctemplates/irm.html:125
#: translation/templates/doctemplates/irm.html:126
msgid "Allaitez-vous?"
msgstr "Do you breastfeed?"

#: translation/templates/doctemplates/irm.html:151
#: translation/templates/doctemplates/irm.html:152
msgid "Avez-vous des clips chirurgicaux intra crâniens (CI avant 1992)?"
msgstr "Do you have intracranial clips (before 1992)?"

#: translation/templates/doctemplates/irm.html:158
#: translation/templates/doctemplates/irm.html:159
msgid "Avez-vous un implant cochléaire?"
msgstr "Do you have a cochlear implant?"

#: translation/templates/doctemplates/irm.html:165
#: translation/templates/doctemplates/irm.html:166
msgid "Avez-vous un implant oculaire?"
msgstr "Do you have an intraocular lens implant?"

#: translation/templates/doctemplates/irm.html:173
#: translation/templates/doctemplates/irm.html:174
msgid "Avez-vous du métal dans le corps?"
msgstr "Do you have any metal in your body?"

#: translation/templates/doctemplates/irm.html:180
#: translation/templates/doctemplates/irm.html:181
#: translation/templates/translation/language.html:3561
#: translation/templates/translation/language.html:3564
msgid "Avez-vous des éclats métalliques dans les yeux?"
msgstr "Do you have any metallic shards in your eyes?"

#: translation/templates/doctemplates/irm.html:188
#: translation/templates/doctemplates/irm.html:189
msgid "Avez-vous des prothèses?"
msgstr "Do you have any prostheses?"

#: translation/templates/doctemplates/irm.html:195
#: translation/templates/doctemplates/irm.html:196
#: translation/templates/doctemplates/scn.html:83
#: translation/templates/doctemplates/scn.html:84
msgid "Êtes-vous enceinte?"
msgstr "Are you pregnant?"

#: translation/templates/doctemplates/irm.html:203
#: translation/templates/doctemplates/irm.html:204
msgid "Aves-vous un capteur de glycémie?"
msgstr "Do you have an implanted blood sugar monitor?"

#: translation/templates/doctemplates/irm.html:210
#: translation/templates/doctemplates/irm.html:211
#: translation/templates/translation/language.html:3585
#: translation/templates/translation/language.html:3588
msgid "Si oui, avez-vous une carte avec les références du matériel?"
msgstr "If yes, do you have a card containing the equipment reference numbers?"

#: translation/templates/doctemplates/irm.html:218
#: translation/templates/doctemplates/irm.html:219
msgid "Avez-vous des allergies?"
msgstr "Do you have any allergies?"

#: translation/templates/doctemplates/irm.html:230
#: translation/templates/doctemplates/irm.html:231
#: translation/templates/doctemplates/scn.html:123
#: translation/templates/doctemplates/scn.html:124
msgid "Signez ici:"
msgstr "Sign here:"

#: translation/templates/doctemplates/scn.html:23
#: translation/templates/doctemplates/scn.html:24
msgid "Vous allez passer une scanner"
msgstr "You are going to have a CT scan"

#: translation/templates/doctemplates/scn.html:26
#: translation/templates/doctemplates/scn.html:27
msgid "Il faudra retirer tout objet métallique (appareils dentaires et auditifs)"
msgstr "Any metallic object (dental and hearing aids) will need to be removed"

#: translation/templates/doctemplates/scn.html:29
#: translation/templates/doctemplates/scn.html:30
msgid "Une perfusion va vous être posée pour injecter un produit de contraste au cours de l’examen. L’injection de ce produit de contraste peut donner une sensation de chaleur dans tout le corps"
msgstr "During the examination, an infusion will be placed to inject a contrast medium. Injection of this contrast medium can give a feeling of warmth throughout the body"

#: translation/templates/doctemplates/scn.html:38
#: translation/templates/doctemplates/scn.html:39
msgid "L'examen dure en moyenne 15 minutes"
msgstr "The examination lasts an average 15 minutes"

#: translation/templates/doctemplates/scn.html:43
msgid "Checklist Scanner"
msgstr "Checklist Scanner"

#: translation/templates/doctemplates/scn.html:90
#: translation/templates/doctemplates/scn.html:91
msgid "Avez-vous des allergies au produit de contraste?"
msgstr "Do you have any allergies to the contrast medium?"

#: translation/templates/doctemplates/scn.html:97
#: translation/templates/doctemplates/scn.html:98
msgid "Avez-vous une insuffisance ou maladie rénale?"
msgstr "Do you have a kidney insufficiency or sickness?"

#: translation/templates/doctemplates/scn.html:104
#: translation/templates/doctemplates/scn.html:105
msgid "Prenez-vous un traitement pour la thyroïde?"
msgstr "Are you being treated for your thyroid?"

#: translation/templates/doctemplates/scn.html:111
#: translation/templates/doctemplates/scn.html:112
msgid "Avez-vous de l'asthme?"
msgstr "Do you have asthma?"

