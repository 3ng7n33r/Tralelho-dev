#: translation/templates/translation/language.html:647
#: translation/templates/translation/language.html:650
msgid "Avez-vous voyagez ces derniers mois ?"
msgstr "Waren Sie in den letzten Monaten verreist?"

#: translation/templates/translation/language.html:655
#: translation/templates/translation/language.html:658
msgid "Dans quel pays ?"
msgstr ""

#: translation/templates/translation/language.html:663
#: translation/templates/translation/language.html:666
msgid "Est ce que la lumière vous dérange ?"
msgstr "Ist Licht Ihnen unangenehm?"

#: translation/templates/translation/language.html:671
#: translation/templates/translation/language.html:674
msgid "Est ce que le bruit vous dérange ?"
msgstr "Ist Lärm Ihnen unangenehm?"

#: translation/templates/translation/language.html:679
#: translation/templates/translation/language.html:682
msgid "Je vais faire une piqûre au bout de votre doigt pour connaître votre taux de sucre"
msgstr "Ich werde das Ende Ihres Fingers anstechen, um Ihren Blutzuckerwert zu ermitteln"

#: translation/templates/translation/language.html:688
msgid "PNEUMOLOGIE"
msgstr ""

#: translation/templates/translation/language.html:694
#: translation/templates/translation/language.html:697
msgid "Je vais poser ma main sur votre ventre pour compter votre respiration, respirez normalement et ne parlez pas pendant ce temps"
msgstr "Ich werde meine Hand auf Ihren Bauch legen, um Ihren Atem zu fühlen. Atmen Sie normal weiter und sprechen Sie während dieser Zeit nicht"

#: translation/templates/translation/language.html:702
#: translation/templates/translation/language.html:705
msgid "Etes-vous essoufflé ?"
msgstr ""

#: translation/templates/translation/language.html:710
#: translation/templates/translation/language.html:713
msgid "Prenez une grande inspiration puis bloquez votre respiration"
msgstr "Atmen Sie tief ein und halten Sie die Luft"

#: translation/templates/translation/language.html:718
#: translation/templates/translation/language.html:721
msgid "Respirez normalement"
msgstr "Atmen Sie normal"

#: translation/templates/translation/language.html:726
#: translation/templates/translation/language.html:729
msgid "Respirez profondément"
msgstr "Atmen Sie tief ein und aus"

#: translation/templates/translation/language.html:734
#: translation/templates/translation/language.html:737
msgid "Fumez-vous ?"
msgstr "Rauchen Sie?"

#: translation/templates/translation/language.html:742
#: translation/templates/translation/language.html:745
msgid "Etes-vous asthmatique ?"
msgstr "Haben Sie Asthma?"

#: translation/templates/translation/language.html:750
#: translation/templates/translation/language.html:753
msgid "Avez vous pris votre traitement contre l’asthme ?"
msgstr "Haben Sie Ihr Medikament gegen Asthma genommen?"

#: translation/templates/translation/language.html:758
#: translation/templates/translation/language.html:761
msgid "Avez-vous respiré des fumées ?"
msgstr "Haben Sie Rauch eingeatmet?"

#: translation/templates/translation/language.html:766
#: translation/templates/translation/language.html:769
msgid "Je vous laisse vous moucher, pour savoir s’il y a des traces de suie"
msgstr "Bitte schneuzen Sie sich die Nase, damit ich Spuren von Ruß erkennen kann"

#: translation/templates/translation/language.html:775
msgid "CARDIOLOGIE"
msgstr ""

#: translation/templates/translation/language.html:781
#: translation/templates/translation/language.html:784
msgid "Est ce que votre douleur serre ?"
msgstr ""

#: translation/templates/translation/language.html:789
#: translation/templates/translation/language.html:792
msgid "Est ce que votre douleur pique ?"
msgstr ""

#: translation/templates/translation/language.html:797
#: translation/templates/translation/language.html:800
msgid "Est ce que votre douleur brûle ?"
msgstr ""

#: translation/templates/translation/language.html:805
#: translation/templates/translation/language.html:808
#: translation/templates/translation/language.html:980
#: translation/templates/translation/language.html:983
#: translation/templates/translation/language.html:1051
#: translation/templates/translation/language.html:1054
msgid "Est-ce que votre douleur irradie ? Montrez-moi vers où"
msgstr ""

#: translation/templates/translation/language.html:813
#: translation/templates/translation/language.html:816
msgid "Depuis combien de temps avez vous cette douleur  ?"
msgstr "Wie lange haben Sie diese Schmerzen?"

#: translation/templates/translation/language.html:821
#: translation/templates/translation/language.html:824
msgid "Minutes"
msgstr "Minuten"

#: translation/templates/translation/language.html:829
#: translation/templates/translation/language.html:832
msgid "Heures"
msgstr "Stunden"

#: translation/templates/translation/language.html:837
#: translation/templates/translation/language.html:840
msgid "Jours"
msgstr "Tage"

#: translation/templates/translation/language.html:845
#: translation/templates/translation/language.html:848
msgid "Je vais prendre votre pouls"
msgstr "Ich werde Ihren Puls messen"

#: translation/templates/translation/language.html:853
#: translation/templates/translation/language.html:856
msgid "Je vais appuyer doucement sur votre ongle"
msgstr "Ich werde leicht auf Ihren Nagel drücken"

#: translation/templates/translation/language.html:861
#: translation/templates/translation/language.html:864
msgid "Je vais prendre votre pression artérielle"
msgstr "Ich werde Ihren Blutdruck messen"

#: translation/templates/translation/language.html:869
#: translation/templates/translation/language.html:872
msgid "Ressentez-vous des palpitations ?"
msgstr ""

#: translation/templates/translation/language.html:877
#: translation/templates/translation/language.html:880
msgid "Ouvrez la bouche et soulevez la langue, je vais vous donner un traitement"
msgstr "Öffnen Sie den Mund und drücken Sie die Zunge gegen den Gaumen. Ich werde Ihnen ein Medikament geben"

#: translation/templates/translation/language.html:885
#: translation/templates/translation/language.html:888
msgid "Est ce que ce médicament soulage votre douleur ?"
msgstr ""

#: translation/templates/translation/language.html:893
#: translation/templates/translation/language.html:896
msgid "Buvez-vous de l'alcool ?"
msgstr "Trinken Sie Alkohol?"

#: translation/templates/translation/language.html:901
#: translation/templates/translation/language.html:904
msgid "Avez-vous du diabète ?"
msgstr "Sind Sie Diabetiker?"

#: translation/templates/translation/language.html:909
#: translation/templates/translation/language.html:912
msgid "Avez-vous du cholestérol ?"
msgstr "Haben Sie hohe Cholesterol-Werte?"

#: translation/templates/translation/language.html:917
#: translation/templates/translation/language.html:920
msgid "Je vais faire un enregistrement de votre coeur, ce n’est pas douloureux, ne bouger pas pendant quelques secondes"
msgstr "Ich werde Ihre Herztöne aufnehmen. Das tut nicht weh, aber halten Sie bitte still für einige Sekunden"

#: translation/templates/translation/language.html:926
msgid "MALAISE"
msgstr ""

#: translation/templates/translation/language.html:932
#: translation/templates/translation/language.html:935
msgid "Avez-vous senti votre malaise arriver ?"
msgstr ""

#: translation/templates/translation/language.html:940
#: translation/templates/translation/language.html:943
msgid "Qu’avez-vous ressenti pendant la malaise ?"
msgstr "Was haben Sie während des Unwohlseins gespürt (Kribbeln, Schmerz, wo)?"

#: translation/templates/translation/language.html:948
#: translation/templates/translation/language.html:951
msgid "Avez-vous ressenti des fourmillements ?"
msgstr ""

#: translation/templates/translation/language.html:956
#: translation/templates/translation/language.html:959
msgid "Avez-vous ressenti des vertiges ?"
msgstr ""

#: translation/templates/translation/language.html:964
#: translation/templates/translation/language.html:967
msgid "Avez-vous ressenti une grande fatigue ?"
msgstr ""

#: translation/templates/translation/language.html:972
#: translation/templates/translation/language.html:975
msgid "Avez vous des vertiges ?"
msgstr ""

#: translation/templates/translation/language.html:988
#: translation/templates/translation/language.html:991
msgid "A quel endroit avez-vous ressenti ces signes ?"
msgstr ""

#: translation/templates/translation/language.html:996
#: translation/templates/translation/language.html:999
msgid "Combien de temps à duré votre malaise ?"
msgstr "Wie lange hat ihr Unwohlsein angehalten?"

#: translation/templates/translation/language.html:1004
#: translation/templates/translation/language.html:1007
msgid "Avez-vous perdu vos urines ?"
msgstr "Hatten Sie Blasenschwäche?"

#: translation/templates/translation/language.html:1012
#: translation/templates/translation/language.html:1015
msgid "Vous êtes-vous mordu la langue ? Ouvrez la bouche"
msgstr "Haben Sie sich auf die Zunge gebissen? Öffnen Sie bitte den Mund"

#: translation/templates/translation/language.html:1020
#: translation/templates/translation/language.html:1023
msgid "Avez vous convulsé ?"
msgstr "Hatten Sie Krämpfe?"

#: translation/templates/translation/language.html:1028
#: translation/templates/translation/language.html:1031
msgid "Avez-vous mangé aujourd’hui ?"
msgstr "Haben Sie heute schon gegessen?"

#: translation/templates/translation/language.html:1037
msgid "DIGESTIF"
msgstr ""

#: translation/templates/translation/language.html:1043
#: translation/templates/translation/language.html:1046
msgid "Montrez moi ou vous avez mal"
msgstr "Zeigen Sie mir, wo es weh tut"

#: translation/templates/translation/language.html:1059
#: translation/templates/translation/language.html:1062
msgid "Avez-vous perdu du poids ces derniers mois ? Combien de Kg ?"
msgstr "Haben Sie in den letzten Monaten an Gewicht verloren? Wie viele Kilos?"

#: translation/templates/translation/language.html:1067
#: translation/templates/translation/language.html:1070
msgid "Avez-vous des brûlures quand vous urinez ?"
msgstr "Brennt es, wenn Sie Wasser lassen?"

#: translation/templates/translation/language.html:1075
#: translation/templates/translation/language.html:1078
msgid "Est-ce qu’il y a du sang dans vos urines ?"
msgstr "Ist Blut in Ihrem Harn?"

#: translation/templates/translation/language.html:1083
#: translation/templates/translation/language.html:1086
msgid "Quelle est la date de vos dernières règles ?"
msgstr "Wann hatten Sie ihre letzte Periode?"

#: translation/templates/translation/language.html:1091
#: translation/templates/translation/language.html:1094
#: translation/templates/translation/language.html:1470
#: translation/templates/translation/language.html:1473
msgid "Etes vous enceinte ?"
msgstr "Sind Sie schwanger?"

#: translation/templates/translation/language.html:1099
#: translation/templates/translation/language.html:1102
msgid "A quelle heure avez-vous uriné pour la dernière fois aujourd’hui ?"
msgstr "Um wieviel Uhr haben Sie zum letzten Mal Wasser gelassen?"

#: translation/templates/translation/language.html:1107
#: translation/templates/translation/language.html:1110
msgid "Êtes vous constipé ?"
msgstr "Haben Sie Verstopfung?"

#: translation/templates/translation/language.html:1115
#: translation/templates/translation/language.html:1118
msgid "Depuis combien de jours ?"
msgstr "Seit wie vielen Tagen?"

#: translation/templates/translation/language.html:1123
#: translation/templates/translation/language.html:1126
msgid "Avez-vous des nausées ?"
msgstr "Ist Ihnen übel?"

#: translation/templates/translation/language.html:1131
#: translation/templates/translation/language.html:1134
msgid "Avez-vous des diarrhées ?"
msgstr "Haben Sie Durchfall?"

#: translation/templates/translation/language.html:1139
#: translation/templates/translation/language.html:1142
msgid "Avez-vous vomi ?"
msgstr "Haben Sie erbrochen?"

#: translation/templates/translation/language.html:1147
#: translation/templates/translation/language.html:1150
msgid "Y a-t-il du sang dans vos selles ?"
msgstr "Haben Sie Blut in Ihrem Stuhl festgestellt?"

#: translation/templates/translation/language.html:1155
#: translation/templates/translation/language.html:1158
msgid "Avez-vous des gazs ?"
msgstr "Haben Sie Blähungen?"

#: translation/templates/translation/language.html:1163
#: translation/templates/translation/language.html:1166
msgid "Je dois vous faire un toucher rectal, êtes vous d’accord ?"
msgstr "Ich muss eine Rektaluntersuchung durchführen. Ist Ihnen das recht?"

#: translation/templates/translation/language.html:1171
#: translation/templates/translation/language.html:1174
msgid "Il faudra uriner dans ce pot pour réaliser un examen"
msgstr "Bitte urinieren Sie in diesem Becher, damit wir Ihren Harn untersuchen können"

#: translation/templates/translation/language.html:1179
#: translation/templates/translation/language.html:1182
msgid "Il faudra réaliser une toilette intime avant d’uriner dans le pot"
msgstr "Bitte waschen Sie Ihren Genitalbereich, bevor Sie in dieses Glas urinieren"

#: translation/templates/translation/language.html:1188
msgid "INFECTIEUX"
msgstr ""

#: translation/templates/translation/language.html:1194
#: translation/templates/translation/language.html:1197
msgid "Avez-vous été piqué ou mordu ?"
msgstr "Wurden Sie gestochen oder gebissen?"

#: translation/templates/translation/language.html:1202
#: translation/templates/translation/language.html:1205
msgid "Montrez moi à quel endroit"
msgstr "Zeigen Sie mir, wo"

#: translation/templates/translation/language.html:1210
#: translation/templates/translation/language.html:1213
msgid "Montrez moi où étaient les premiers boutons"
msgstr "Zeigen Sie mir, wo die ersten Flecken aufgetreten sind"

#: translation/templates/translation/language.html:1218
#: translation/templates/translation/language.html:1221
msgid "Depuis combien de temps la jambe est-elle rouge ?"
msgstr "Wie lange ist das Bein schon gerötet?"

#: translation/templates/translation/language.html:1226
#: translation/templates/translation/language.html:1229
msgid "Est ce que ça vous démange ?"
msgstr "Juckt es Sie?"

#: translation/templates/translation/language.html:1234
#: translation/templates/translation/language.html:1237
msgid "Il faut rester en isolement"
msgstr "Sie müssen in Isolation bleiben"

#: translation/templates/translation/language.html:1242
#: translation/templates/translation/language.html:1245
msgid "Vous devez garder ce masque sur vous"
msgstr "Sie müssen diese Schutzmaske tragen"

#: translation/templates/translation/language.html:1250
#: translation/templates/translation/language.html:1253
msgid "Avez-vous eu des rapports sexuels non protégés ?"
msgstr "Haben Sie ungeschützten Geschlechtsverkehr gehabt?"

#: translation/templates/translation/language.html:1258
#: translation/templates/translation/language.html:1261
msgid "Je vais prendre votre température"
msgstr "Ich werde Ihre Körpertemperatur messen."

#: translation/templates/translation/language.html:1267
msgid "OPHTALMOLOGIE"
msgstr ""

#: translation/templates/translation/language.html:1273
#: translation/templates/translation/language.html:1276
msgid "Est-ce que vous voyez flou ?"
msgstr "Sehen Sie verschwommen?"

#: translation/templates/translation/language.html:1281
#: translation/templates/translation/language.html:1284
msgid "Est-ce que vous voyez double ?"
msgstr "Sehen Sie doppelt?"

#: translation/templates/translation/language.html:1289
#: translation/templates/translation/language.html:1292
msgid "Avez-vous mal à la tête ?"
msgstr "Haben Sie Kopfschmerzen?"

#: translation/templates/translation/language.html:1297
#: translation/templates/translation/language.html:1300
msgid "Est ce que vous avez l’impression que la pièce tourne autour de vous ?"
msgstr ""

#: translation/templates/translation/language.html:1305
#: translation/templates/translation/language.html:1308
msgid "Avez-vous reçu un coup à la tête récemment ?"
msgstr ""

#: translation/templates/translation/language.html:1314
msgid "ANTÉCÉDENTS"
msgstr ""

#: translation/templates/translation/language.html:1320
#: translation/templates/translation/language.html:1323
msgid "Avez-vous des maladies (diabète, hypertension) ?"
msgstr ""

#: translation/templates/translation/language.html:1328
#: translation/templates/translation/language.html:1331
msgid "Avez-vous été hospitalisé récemment ?"
msgstr ""

#: translation/templates/translation/language.html:1336
#: translation/templates/translation/language.html:1339
msgid "Avez-vous un traitement en cours ? lequel ?"
msgstr ""

#: translation/templates/translation/language.html:1344
#: translation/templates/translation/language.html:1347
msgid "Avez-vous une ordonnance avec vos traitements ?"
msgstr ""

#: translation/templates/translation/language.html:1352
#: translation/templates/translation/language.html:1355
msgid "Avez-vous des allergies ? lesquelles ?"
msgstr ""

#: translation/templates/translation/language.html:1360
#: translation/templates/translation/language.html:1363
msgid "Y a-t-il des antécédents de maladie dans votre famille ?"
msgstr ""

#: translation/templates/translation/language.html:1368
#: translation/templates/translation/language.html:1371
msgid "Pouvez-vous vous asseoir ?"
msgstr ""

#: translation/templates/translation/language.html:1376
#: translation/templates/translation/language.html:1379
msgid "Pouvez-vous vous lever ?"
msgstr ""

#: translation/templates/translation/language.html:1384
#: translation/templates/translation/language.html:1387
msgid "Pouvez-vous marcher ?"
msgstr ""

#: translation/templates/translation/language.html:1393
msgid "PEDIATRIE"
msgstr ""

#: translation/templates/translation/language.html:1399
#: translation/templates/translation/language.html:1402
msgid "Le bébé a t il perdu du poids ? Combien de kilo ?"
msgstr "Hat das Baby an Gewicht verloren? Wie viele Kilo?"

#: translation/templates/translation/language.html:1407
#: translation/templates/translation/language.html:1410
msgid "Est-ce que le bébé est à jour de ces vaccins ?"
msgstr "Sind alle Impfungen des Babys vollständig und aktuell?"

#: translation/templates/translation/language.html:1415
#: translation/templates/translation/language.html:1418
msgid "Est-ce que ses frères et soeurs sont malades ?"
msgstr "Sind Ihre anderen Kinder krank?"

#: translation/templates/translation/language.html:1423
#: translation/templates/translation/language.html:1426
msgid "Est-ce qu’il mange bien ?"
msgstr "Isst er/sie gut?"

#: translation/templates/translation/language.html:1431
#: translation/templates/translation/language.html:1434
msgid "Est-ce qu’il vomi ?"
msgstr "Erbricht er/sie?"

#: translation/templates/translation/language.html:1439
#: translation/templates/translation/language.html:1442
msgid "Est-ce qu’il semble plus agité que d’habitude ?"
msgstr "Scheint er/sie aufgeregter als gewöhnlich?"

#: translation/templates/translation/language.html:1447
#: translation/templates/translation/language.html:1450
msgid "Est-ce qu’il semble plus fatigué que d’habitude ?"
msgstr "Scheint er/sie müder als gewöhnlich?"

#: translation/templates/translation/language.html:1455
#: translation/templates/translation/language.html:1458
msgid "Est ce qu’il a la diarrhée ?"
msgstr "Hat er/sie Durchfall?"

#: translation/templates/translation/language.html:1464
msgid "GYNECOLOGIE"
msgstr ""

#: translation/templates/translation/language.html:1478
#: translation/templates/translation/language.html:1481
msgid "De combien de semaines ?"
msgstr "In welcher Woche?"

#: translation/templates/translation/language.html:1486
#: translation/templates/translation/language.html:1489
msgid "Avez vous vos règles ?"
msgstr "Haben Sie Ihre Monatsblutung?"

#: translation/templates/translation/language.html:1494
#: translation/templates/translation/language.html:1497
msgid "Avez vous perdu du sang ?"
msgstr "Haben Sie Blut verloren?"

#: translation/templates/translation/language.html:1502
#: translation/templates/translation/language.html:1505
msgid "Vos pertes étaient elles rouge ou noir ?"
msgstr "War das Blut rot oder schwarz?"

#: translation/templates/translation/language.html:1510
#: translation/templates/translation/language.html:1513
msgid "Avez vous eu des problèmes lors de vos précédentes grossesses ?"
msgstr "Hatten Sie Probleme während Ihrer vorherigen Schwangerschaften?"

#: translation/templates/translation/language.html:1518
#: translation/templates/translation/language.html:1521
msgid "Avez vous des contractions ?"
msgstr "Haben Sie Verspannungen?"

#: translation/templates/translation/language.html:1526
#: translation/templates/translation/language.html:1529
msgid "Avez vous perdu les eaux ?"
msgstr "Haben Sie Wasser verloren?"

#: translation/templates/translation/language.html:1534
#: translation/templates/translation/language.html:1537
msgid "Sentez vous le bébé bouger ?"
msgstr "Fühlen Sie die Bewegungen des Babys?"

#: translation/templates/translation/language.html:1542
#: translation/templates/translation/language.html:1545
msgid "Avez vous une contraception ?"
msgstr "Benutzen Sie Empfängnisverhütungsmittel?"

#: translation/templates/translation/language.html:1550
#: translation/templates/translation/language.html:1553
msgid "Je dois faire un examen gynécologique, allongez vous sur la table"
msgstr "Ich muss eine gynäkologische Untersuchung durchführen. Bitte legen Sie sich auf den Tisch"

#: translation/templates/translation/language.html:1558
#: translation/templates/translation/language.html:1561
msgid "Il faut retirer vos sous-vêtements"
msgstr "Bitte ziehen Sie Ihre Unterwäsche aus"

#: translation/templates/translation/language.html:1567
msgid "TRAUMATOLOGIE"
msgstr ""

#: translation/templates/translation/language.html:1573
#: translation/templates/translation/language.html:1576
msgid "Avez-vous été éjecté du véhicule ?"
msgstr "Wurden Sie aus dem Fahrzeug geschleudert?"

#: translation/templates/translation/language.html:1581
#: translation/templates/translation/language.html:1584
msgid "A quelle vitesse rouliez-vous ?"
msgstr "Wie schnell sind Sie gefahren?"

#: translation/templates/translation/language.html:1589
#: translation/templates/translation/language.html:1592
msgid "Etiez-vous casqué ?"
msgstr "Haben Sie einen Helm getragen?"

#: translation/templates/translation/language.html:1597
#: translation/templates/translation/language.html:1600
msgid "Aviez-vous votre ceinture de sécurité ?"
msgstr "War Ihr Sicherheitsgurt angelegt?"

#: translation/templates/translation/language.html:1605
#: translation/templates/translation/language.html:1608
msgid "Avez vous fait une chute ?"
msgstr "Sind Sie gestürzt?"

#: translation/templates/translation/language.html:1613
#: translation/templates/translation/language.html:1616
msgid "De quelle hauteur êtes-vous tombé ?"
msgstr "Aus welcher Höhe sind Sie gefallen?"

#: translation/templates/translation/language.html:1621
#: translation/templates/translation/language.html:1624
msgid "Prenez vous des anticoagulants, des médicaments pour fluidifier le sang ?"
msgstr "Nehmen Sie gerinnungshemmende Medikamente oder Blutverdünner?"

#: translation/templates/translation/language.html:1629
#: translation/templates/translation/language.html:1632
msgid "Je vais installer une collier afin de protéger votre colonne vertébrale"
msgstr "Ich werde Ihnen eine Halschiene anlegen, um Ihre Wirbelsäule zu schützen"

#: translation/templates/translation/language.html:1637
#: translation/templates/translation/language.html:1640
msgid "Je dois mettre un antiseptique sur la plaie"
msgstr "Ich muss die Wunde desinfizieren"

#: translation/templates/translation/language.html:1645
#: translation/templates/translation/language.html:1648
msgid "Je dois suturer la plaie"
msgstr "Ich muss die Wunde nähen"

#: translation/templates/translation/language.html:1653
#: translation/templates/translation/language.html:1656
msgid "Je dois faire un pansement"
msgstr "Ich muss einen Verband anlegen"

#: translation/templates/translation/language.html:1661
#: translation/templates/translation/language.html:1664
msgid "Je vais faire une anesthésie autour de la plaie"
msgstr "Ich werde das Umfeld der Wunde lokal betäuben"

#: translation/templates/translation/language.html:1669
#: translation/templates/translation/language.html:1672
msgid "Ne bougez pas"
msgstr "Bitte bewegen Sie sich nicht"

#: translation/templates/translation/language.html:1677
#: translation/templates/translation/language.html:1680
msgid "Je dois vous faire un plâtre"
msgstr "Ich muss Ihnen einen Gipsverband machen"

#: translation/templates/translation/language.html:1685
#: translation/templates/translation/language.html:1688
#: translation/templates/translation/language.html:2034
#: translation/templates/translation/language.html:2037
msgid "Vous devez être opéré"
msgstr "Sie müssen operiert werden"

#: translation/templates/translation/language.html:1694
msgid "EXAMENS"
msgstr ""

#: translation/templates/translation/language.html:1700
#: translation/templates/translation/language.html:1703
msgid "Je vais vous poser une perfusion"
msgstr ""

#: translation/templates/translation/language.html:1708
#: translation/templates/translation/language.html:1711
msgid "Je vais vous faire une prise de sang"
msgstr ""

#: translation/templates/translation/language.html:1716
#: translation/templates/translation/language.html:1719
msgid "Je vais vous faire un pansement"
msgstr "Ich werde Sie verbinden"

#: translation/templates/translation/language.html:1724
#: translation/templates/translation/language.html:1727
msgid "Je vais vous faire une piqûre"
msgstr "Ich werde Ihnen eine Spritze geben"

#: translation/templates/translation/language.html:1732
#: translation/templates/translation/language.html:1735
msgid "Je vais faire votre toilette"
msgstr "Ich werde Sie waschen"

#: translation/templates/translation/language.html:1740
#: translation/templates/translation/language.html:1743
msgid "Vous allez avoir un scanner"
msgstr ""

#: translation/templates/translation/language.html:1748
#: translation/templates/translation/language.html:1751
msgid "Vous allez avoir une radio"
msgstr ""

#: translation/templates/translation/language.html:1756
#: translation/templates/translation/language.html:1759
msgid "Je vais vous faire un plâtre"
msgstr "Ich muss Ihnen einen Gipsverband machen"

#: translation/templates/translation/language.html:1764
#: translation/templates/translation/language.html:1767
msgid "Vous allez avoir une échographie"
msgstr ""

